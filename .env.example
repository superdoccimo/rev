# --- Nginx reverse proxy vars ---
UPSTREAM=http://10.0.0.37:8080
DOMAINS=example.com www.example.com
EMAIL=you@example.com
STAGING=1   # 1=staging, 0=production

# --- Cloudflare DNS-01 (optional) ---
# Put the real token in certbot/conf/cloudflare.ini (not in .env)
# dns_cloudflare_api_token = <YOUR_API_TOKEN>

# --- Paths (override if needed) ---
LE_CONF=./certbot/conf
LE_WEBROOT=./certbot/www
NGINX_CONTAINER=proxy-nginx

